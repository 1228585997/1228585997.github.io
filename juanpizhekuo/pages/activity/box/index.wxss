}

.act-goods {
    padding-bottom: 100rpx;
}

.goods-txt {
    width: 750rpx;
    height: 100rpx;
    margin-bottom: 32rpx;
}

.goods-info {
    padding: 0 10rpx 0 20rpx;
    overflow: hidden;
    zoom: 1;
    clear: both;
}

.goods-info .goods-list {
    background: #fff;
    float: left;
    margin: 0 10rpx 10rpx 0;
    width: 350rpx;
    height: 440rpx;
}

.goods-list .goods-pic {
    width: 350rpx;
    height: 350rpx;
    position: relative;
    margin-bottom: 15rpx;
}

.goods-pic .pic {
    display: block;
    width: 350rpx;
    height: 350rpx;
}

.goods-list .goods-title {
    display: -webkit-box;
    color: #333;
    font-size: 24rpx;
    font-weight: bold;
    padding: 0 15rpx;
    word-break: break-all;
    text-overflow: ellipsis;
    overflow: hidden;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 1;
    margin-bottom: 3rpx;
}

.new-price {
    display: inline-block;
    color: #f72f4c;
    font-size: 30rpx;
    font-weight: bold;
