image {
    width: 100%;
    height: 100%;
    display: block;
}

.ListOuter {
    width: 100%;
    background-color: #e5304c;
}

.ListTopImg {
    width: 100%;
    height: 239rpx;
}

.ListCenter {
    width: 700rpx;
    height: 1194rpx;
    padding: 30rpx 26rpx;
    box-sizing: border-box;
    border-radius: 40rpx;
    background-color: #d7233f;
    margin-left: 50%;
    transform: translateX(-50%);
}

.ListCenterInner {
    width: 100%;
    height: 100%;
    padding: 20rpx 34rpx;
    box-sizing: border-box;
    border-radius: 40rpx;
    background-color: #f7f4ff;
}

.ListCenterTitle {
    width: 100%;
    height: 44rpx;
    background-color: #f7f4ff;
    font-size: 24rpx;
    text-align: center;
    color: #e5304c;
    border-bottom: 4rpx solid #e5304c;
    display: flex;
}

.ListCenterTitle .span {
    flex: 1.5;
}

.ListCenterTitle .em {
    flex: 1;
}

.ListCenterUl {
    width: 100%;
    height: calc(100% - 44rpx);
}

.ListCenterLi {
    width: 100%;
    height: 52rpx;
    background-color: #f7f4ff;
    font-size: 24rpx;
    text-align: center;
    color: #999;
    border-bottom: 2rpx dashed #999;
    display: flex;
    align-items: center;
}

.ListCenterHint {
    padding-top: 20rpx;
    text-align: center;
}

.ListCenterLi .span {
    flex: 1.5;
}

.ListCenterLi .em {
    flex: 1;
}

.ListHint {
    padding: 8rpx 24rpx 30rpx;
    box-sizing: border-box;
    color: #ffebc0;
    font-size: 22rpx;
}

.ListHint text {
    display: block;
}

.ListHint .h3 {
    font-size: 30rpx;
    line-height: 60rpx;
}

.imageFoot {
    width: 100%;
    height: 200rpx;
}