.turnplate-page {
    width: 100%;
    position: relative;
    background-color: #cb004c;
    color: #ffeeea;
    text-align: center;
    font-size: 24rpx;
    overflow: hidden;
}

.turnplate-alert {
    position: fixed;
    width: 100%;
    height: 100%;
    background-color: rgba(0,0,0,.5);
    z-index: 999;
    left: 0;
    top: 0;
}

.turnplate-alertBox {
    width: 274px;
    position: absolute;
    left: 50%;
    top: 50%;
    transform: translate(-50%,-50%);
    box-sizing: border-box;
    background-color: #fff;
    background-image: url('data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAAoAAAABCAYAAADn9T9+AAAAF0lEQVQImWP8////fwZcYDojhM78zwAAo3gF/mXdkL0AAAAASUVORK5CYII=');
    background-position: top left;
    background-repeat: repeat-y;
    background-size: 5px;
}

.turnplate-alertBox .turnplate-alertBox-topBoder {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 15.5px;
    background: url('data:image/png;base64,');
    background-size: 100%;
}

.turnplate-alertBox .turnplate-alertBox-btmBoder {
    position: absolute;
    bottom: 0;
    left: 0;
    width: 100%;
    height: 15.5px;
    background: url('data:image/png;base64,');
    background-size: 100%;
}

.turnplate-alertBox .turnplate-alertBox-close {
    width: 34rpx;
    height: 34rpx;
    position: absolute;
    right: 20rpx;
    top: 20rpx;
    background: url('data:image/png;base64,');
    background-size: 100% 100%;
}

.turnplate-alertBox .turnplate-alertBox-inner {
    background-image: url('data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAAoAAAABCAYAAADn9T9+AAAAG0lEQVQImWP8////f4bpjAwMDAwMDJn/GXABANN4Bf4vSpZOAAAAAElFTkSuQmCC');
    background-position: top right;
    background-repeat: repeat-y;
    background-size: 5px;
    padding: 20px 15px;
}

.turnplate-alertBox .turnplate-alertBox-inner .turnplate-alertBox-rule {
    color: #ff9600;
    font-size: 24rpx;
    text-align: left;
    text-indent: 24rpx;
}

.turnplate-alertBox .turnplate-alertBox-inner .turnplate-alertBox-rule .rule-title {
    text-align: center;
    font-size: 28rpx;
    line-height: 40rpx;
}

.turnplate-alertBox .turnplate-alertBox-lucky {
    min-height: 174rpx;
    display: flex;
    flex-direction: column;
    justify-content: space-between;
    align-items: center;
}

.turnplate-alertBox .turnplate-alertBox-lucky image {
    width: 115rpx;
    height: 121rpx;
}

.turnplate-alertBox .turnplate-alertBox-lucky text {
    font-size: 32rpx;
    color: #ff9600;
}

.turnplate-alertBox .turnplate-alertBox-sorry {
    justify-content: center;
}

.turnplate-alertBox .turnplate-alertBox-lucky .turnplate-alertBox-h2 {
    font-size: 36rpx;
    color: #919191;
}

.turnplate-alertBox .turnplate-alertBox-lucky .turnplate-alertBox-h3 {
    font-size: 28rpx;
    color: #919191;
}

.turnplate-alertBox .turnplate-alertBox-btns {
    display: flex;
    justify-content: center;
    height: 85rpx;
    padding-top: 20rpx;
}

.turnplate-alertBox .turnplate-alertBox-btn {
    display: flex;
    width: 170rpx;
    height: 100%;
    text-align: center;
    color: #fff;
    background-color: #ff5328;
    border-radius: 4rpx;
    font-size: 28rpx;
    justify-content: center;
    align-items: center;
    padding: 0;
}

.turnplate-alertBox .turnplate-alertBox-btn:nth-child(2) {
    margin-left: 32rpx;
}

.turnplate-alertBox .turnplate-alertBox-btn text {
    width: 16rpx;
    height: 16rpx;
    box-sizing: border-box;
    border-bottom: 4rpx solid #fff;
    border-right: 4rpx solid #fff;
    transform: rotate(-45deg);
}

.turnplate-alertBox .turnplate-alertBox-bigBtn {
    width: 300rpx;
}

.turnplate-topBox {
    position: relative;
    width: 100%;
    height: 1181rpx;
    background: url('https://web.juanpi.com/images/turnplate/bg.png') no-repeat;
    background-size: 100% 100%;
    padding-top: 208rpx;
    box-sizing: border-box;
}

.turnplate-topBox .turnplate-rule {
    position: absolute;
    top: 20rpx;
    right: 20rpx;
    width: 100rpx;
    height: 40rpx;
    background-color: rgba(0,0,0,.3);
    border-radius: 50rpx;
    display: flex;
    justify-content: center;
    align-items: center;
    font-size: 22rpx;
}

.turnplate-topBox .turnplate-rule text {
    font-size: 20rpx;
    width: 26rpx;
    height: 26rpx;
    line-height: 20rpx;
    border: 1px solid #fff;
    border-radius: 100%;
    margin-right: 8rpx;
}

.turnplate-topBox .turnplate-date {
    color: #fae58e;
    font-size: 26rpx;
    line-height: 50rpx;
    font-style: oblique;
}

.turnplate-topBox .turnplate-prizeHint {
    height: 83rpx;
    color: #fff;
    background: url('https://goods7.juancdn.com/act/180116/c/d/5a5d9e5d8150a17c643d7bea_623x83.png') no-repeat center center;
    background-size: 623rpx;
    line-height: 56rpx;
    text-shadow: 0rpx 4rpx 8rpx rgba(0,0,0,.5);
}

.turnplate-topBox .turnplate-lucky {
    height: 660rpx;
    position: relative;
}

.turnplate-topBox .turnplate-lucky .turnplate-luckyBox {
    width: 660rpx;
    height: 660rpx;
}

.turnplate-topBox .turnplate-lucky .turnplate-luckyBtn {
    width: 231rpx;
    height: 270rpx;
    background: url('https://goods4.juancdn.com/act/180116/6/4/5a5da331a9fcf873f341f102_231x270.png');
    background-size: 100% 100%;
    position: absolute;
    left: 50%;
    top: 50%;
    transform: translate(-50%,-50%);
}

.turnplate-topBox .turnplate-numberHint {
    font-size: 30rpx;
    line-height: 80rpx;
}

.turnplate-topBox .turnplate-numberHint text {
    color: #fffd39;
}

.turnplate-topBox .turnplate-btnBox {
    height: 96rpx;
    text-align: center;
    font-size: 0;
}

.turnplate-topBox .turnplate-btnBox .turnplate-btn {
    display: inline-block;
    margin: 0;
    padding: 0;
    width: 304rpx;
    line-height: 96rpx;
    color: #fff;
    background: url('https://goods8.juancdn.com/act/180116/f/5/5a5dab328150a17f084958c3_304x96.png');
    background-size: 100% 100%;
    font-size: 36rpx;
    vertical-align: middle;
}

.turnplate-topBox .turnplate-btnBox .turnplate-btn .angle {
    width: 20rpx;
    height: 20rpx;
}

.turnplate-midBox {
    width: 100%;
    height: 424rpx;
    padding: 20rpx 20rpx 0;
    box-sizing: border-box;
}

.turnplate-midBox .turnplate-msgBox {
    width: 100%;
    height: 100%;
    border-radius: 20rpx;
    background-color: #ff604b;
    border: 1px solid #f5aa19;
    box-sizing: border-box;
    overflow: hidden;
    box-shadow: 0 8px 16px rgba(0,0,0,.2);
}

.turnplate-midBox .turnplate-msgBox .turnplate-msgBox-header {
    display: flex;
    align-items: center;
    height: 72rpx;
    background-color: #ff604b;
    border-radius: 20rpx 20rpx 0 0;
    color: #fff;
    overflow: hidden;
}

.turnplate-midBox .turnplate-msgBox .turnplate-msgBox-header view {
    flex: 1;
    font-size: 36rpx;
    height: 100%;
    line-height: 72rpx;
    background-color: #f5aa19;
}

.turnplate-midBox .turnplate-msgBox .turnplate-msgBox-header .turnplate-msgBox-active {
    color: #961f08;
    background-color: #ff604b;
}

.turnplate-msgBox-body {
    width: 100%;
    height: 328rpx;
    background: #fffbf0;
    border: 8rpx solid #ff604b;
    border-top: none;
    padding: 0 8rpx;
    box-sizing: border-box;
    border-radius: 0 0 20rpx 20rpx;
    font-size: 26rpx;
}

.turnplate-msgBox-body .turnplate-msgBox-body-header {
    height: 68rpx;
    border-bottom: 1px dashed #8a8a8a;
    color: #8a8a8a;
    display: flex;
    align-items: center;
}

.turnplate-msgBox-body .turnplate-msgBox-body-Ul {
    height: 250rpx;
    line-height: 48rpx;
    color: #ff604b;
}

.turnplate-msgBox-body .turnplate-msgBox-body-Ul .turnplate-msgBox-body-li {
    display: flex;
    align-items: center;
}

.turnplate-msgBox-body-empty {
    color: #8a8a8a;
    padding-top: 136rpx;
}

.turnplate-msgBox-body .turnplate-msgBox-body-header text,.turnplate-msgBox-body .turnplate-msgBox-body-Ul .turnplate-msgBox-body-li text,.turnplate-msgBox-body .turnplate-msgBox-body-Ul .turnplate-msgBox-body-li navigator {
    display: -webkit-inline-box;
    flex: 1;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 1;
    overflow: hidden;
}

.turnplate-btmBox .turnplate-more {
    height: 120rpx;
    background: url('https://goods8.juancdn.com/act/180118/e/f/5a6004e08150a16c4f1a2e82_750x120.png');
    background-size: 100% 100%;
}

.turnplate-btmBox .turnplate-goHome {
    height: 200rpx;
    background: url('https://goods5.juancdn.com/act/180118/9/b/5a60062e8150a16c9a164815_750x200.png');
    background-size: 100% 100%;
}

.turnplate-btmBox .turnplate-footLogo {
    height: 200rpx;
    background: url('https://goods6.juancdn.com/act/180116/a/0/5a5d97358150a17de80f6283_750x200.png');
    background-size: 100% 100%;
}