<template name="search-result">
    <view class="search-result {{quickSortFilter.length>0&&'fixed'}}" hidden="{{hideSearchBar||hidden}}" style="top: 40px;">
        <template is="sort-tabs" data="{{goodsTabs:goodsTabs,goodsList:goodsList}}"></template>
    </view>
    <template is="quick-sort" data="{{quickSortCatesWrapperOpen:quickSortCatesWrapperOpen,hideSearchBar:hideSearchBar,hidden:hidden,cates:cates,activeCateIndex:activeCateIndex,quickSortFilter:quickSortFilter,goodsList:goodsList,activeCateText:activeCateText}}" wx:if="{{quickSortFilter.length>0}}"></template>
    <scroll-view enableBackToTop bindscroll="handleScroll" bindscrolltolower="scrollGetGoods" class="goods-wrap" hidden="{{hidden}}" scrollTop="{{scrollTop}}" scrollY="true" style="height:{{goodsheight}}px">
        <view class="quick-sort-holder" wx:if="{{quickSortFilter.length>0}}"></view>
        <view>
            <template is="goods" data="{{list:goodsList,isVip:isVip}}"></template>
            <include src="/components/loadmore1/loadmore.wxml"></include>
        </view>
    </scroll-view>
</template>
<import src="../../../components/goods/sort-tabs/sort-tabs.wxml"></import>
<import src="../../../components/goods/quick-sort/quick-sort.wxml"></import>
<import src="../../../components/goods/goods.wxml"></import>
<import src="/components/goods-list/index.wxml"></import>
