                <image lazyLoad="true" mode="widthFix" src="{{pFloors[gFloorsIdx].banner.src}}" style="width:100%;height:120rpx;"></image>
            </view>
            <view class="image-box" wx:for="{{pFloors[gFloorsIdx].areas}}" wx:for-index="areasIdx" wx:for-item="areasItem" wx:key="areasIdx">
                <block wx:if="{{areasItem.area.src}}">
