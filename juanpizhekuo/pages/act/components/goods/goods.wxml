                    <block wx:for="{{areasItem.area.anchors}}" wx:for-index="anchorsIdx" wx:for-item="anchorsItem" wx:key="anchorsIdx">
                        <navigator redirect hoverClass="none" style="display:inline-block;position:absolute;width:{{anchorsItem.width}}rpx;height:{{anchorsItem.height}}rpx;top:{{anchorsItem.top}}rpx;left:{{anchorsItem.left}}rpx;" url="{{anchorsItem.href}}" wx:if="{{anchorsItem.type>0}}"></navigator>
                        <view bindtap="ruleTap" data-id="{{anchorsItem.id}}" style="display:inline-block;position:absolute;width:{{anchorsItem.width}}rpx;height:{{anchorsItem.height}}rpx;top:{{anchorsItem.top}}rpx;left:{{anchorsItem.left}}rpx;" wx:else></view>
                    </block>
                </block>
            </view>
            <view class="goods-list gap{{gap}}">
                <template is="{{goodsItem.goodStyle}}" data="{{goodsItem:goodsItem,pFloors:pFloors,gFloorsIdx:gFloorsIdx}}" wx:for="{{gFloorsItem.goodsList}}" wx:for-index="goodsIdx" wx:for-item="goodsItem" wx:key="goodsIdx"></template>
            </view>
        </view>
    </view>
