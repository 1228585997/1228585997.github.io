page,.container {
    height: 100%;
    background-color: #ffb72a;
}

.container {
    box-sizing: border-box;
}

.top_section {
    width: 750rpx;
    height: 350rpx;
}

.top_section_bg {
    width: 100%;
}

.top_coupon_bg {
    width: 592rpx;
    height: 234rpx;
}

.top_coupon {
    width: 592rpx;
    height: 234rpx;
    position: absolute;
    top: 116rpx;
    left: 50%;
    transform: translateX(-50%);
    display: flex;
    color: #fff;
}

.top_coupon_num {
    display: block;
    font-size: 164rpx;
    position: absolute;
    text-align: left;
    top: 51%;
    transform: translateY(-50%);
    letter-spacing: -10rpx;
    word-spacing: -10rpx;
    font-family: "Helvetica Neue",Helvetica,"SF Pro SC","HanHei SC","SF Pro Text","Myriad Set Pro",Arial,sans-serif;
}

.n_type {
    font-size: 60rpx;
    display: block;
    position: absolute;
    top: 64%;
    transform: translateY(-50%);
    left: 30rpx;
}

.top_coupon_num.small {
    left: 90rpx;
}

.top_coupon_num.normal {
    left: 58rpx;
}

.floor {
    width: 750rpx;
    height: 120rpx;
    line-height: 120rpx;
    text-align: center;
    position: relative;
}

.floor_icon {
    width: 64rpx;
    height: 40rpx;
    vertical-align: middle;
    display: inline-block;
}

.floor_text {
    color: #fff;
    font-size: 38rpx;
    vertical-align: middle;
    margin: 0 20rpx;
}

.limit_text {
    display: block;
    position: absolute;
    top: 38rpx;
    left: 50%;
    transform: translateX(-50%);
    font-size: 22rpx;
    color: #ff0000;
}

.isShowTop {
    position: absolute;
    width: 100%;
    top: 0;
    left: 0;
    z-index: -1;
}

.common_list {
    margin-top: -18rpx;
}

.common_coupon {
    width: 345rpx;
    height: 120rpx;
    display: inline-block;
    margin-left: 22rpx;
    margin-top: 18rpx;
    position: relative;
}

.coupon_img {
    width: 100%;
}

.common_action {
    position: absolute;
    top: 20rpx;
    right: 36rpx;
    border-radius: 8rpx;
    color: #fff;
    font-size: 28rpx;
    padding: 8rpx 36rpx;
}

.common_action.type_1 {
    background: #a09f9f;
    padding: 8rpx 14rpx;
}

.common_action.type_2 {
    background: #a09f9f;
}

.common_action.type_3 {
    background: #a09f9f;
}

.common_action.type_4 {
    background-image: linear-gradient(to right,#ff902d,#ff4144);
    padding: 8rpx 22rpx;
}

.common_action.type_5 {
    background-image: linear-gradient(to right,#f93593,#8d35df);
    padding: 8rpx 22rpx;
}

.item_sec {
    padding: 0 24rpx 32rpx 24rpx;
    display: flex;
    flex-wrap: wrap;
    justify-content: space-between;
}

.special-item {
    text-align: center;
}

.s_main_item {
    width: 230rpx;
    height: 260rpx;
    display: block;
    margin-top: 6rpx;
}

.s_main_item image {
    width: 100%;
}

.s_spec_item {
    width: 170rpx;
    height: 230rpx;
    display: block;
    margin-top: 10rpx;
}

.s_spec_item image {
    width: 100%;
}

.logo {
    width: 100rpx;
    height: 100rpx;
    margin: 44rpx 0;
}

.feijian {
    width: 96rpx;
    height: 88rpx;
    display: block;
    position: absolute;
    bottom: 18rpx;
    right: -68rpx;
}

.feijian1 {
    width: 36rpx;
    height: 28rpx;
    display: block;
    position: absolute;
    top: -22rpx;
    left: 136rpx;
}

.bottom_logo {
    background: #ff9c00;
}