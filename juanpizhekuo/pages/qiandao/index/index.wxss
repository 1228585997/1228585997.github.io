page,.container {
    height: 100%;
    background-color: #efeff4;
}

.main {
    padding: 14px 0 24px;
    background: #fff;
}

.item-title {
    padding: 14px 0;
    font-size: 0;
    text-align: center;
    position: relative;
}

.item-title-text {
    display: inline-block;
    position: relative;
    padding: 0 8px;
    line-height: 20px;
    font-size: 13px;
}

.item-title-text::before,.item-title-text::after {
    position: absolute;
    content: '';
    top: 50%;
    height: 2px;
    width: 8px;
    background-color: #333;
    transform: translate(0,-50%);
}

.item-title-text::before {
    left: -8px;
}

.item-title-text::after {
    right: -8px;
}

.bag {
    position: relative;
    font-size: 0;
    text-align: center;
    overflow: hidden;
}

.bag-bg {
    display: block;
    margin: 0 auto;
    max-width: 100%;
    height: auto;
}

.bag-tips {
    margin: 0 auto 14rpx;
    display: flex;
    justify-content: center;
    align-items: center;
    font-size: 0;
}

.bag-tips-text {
    padding: 0 10px;
    font-size: 14px;
    color: #333;
}

.bag-tips-img {
    height: 12px;
    width: 32px;
}

.btn {
    position: relative;
    display: block;
    margin: 0 auto;
    font-size: 0;
    height: 104rpx;
    padding: 12rpx;
    border: 0 none;
    border-radius: 200rpx;
    background: rgba(255,255,255,.2);
    box-sizing: border-box;
    outline: 0 none;
    overflow: hidden;
}

.btn::after {
    display: none;
}

.btn-text {
    display: block;
    height: 80rpx;
    line-height: 80rpx;
    font-size: 32rpx;
    color: #bb0000;
    background: #fff;
    border-radius: 200rpx;
    text-align: center;
    overflow: hidden;
}

.main-title-1 {
    margin: 0 auto;
    position: relative;
    display: inline-block;
    padding: 0 10px;
}

.main-title-1::before,.main-title-1::after {
    content: '';
    position: absolute;
    top: 50%;
    height: 1px;
    width: 20px;
    background: #ffbbbb;
    transform: translate(0,-50%);
}

.main-title-1::before {
    left: -20px;
}

.main-title-1::after {
    right: -20px;
}

.main-title-text {
    font-size: 32rpx;
    color: #fff;
}

.in .main-title-1 {
    margin-bottom: 94rpx;
}

.in.over .main-title-1 {
    margin-bottom: 140rpx;
}

.in.over .bottom-tips {
    margin-bottom: 44rpx;
}

.in.fiveall .main-title-1 {
    margin-bottom: 74rpx;
}

.in.fiveover .main-title-1 {
    margin-bottom: 76rpx;
}

.in.fiveover .bottom-tips {
    margin-bottom: 44rpx;
}

.out {
    position: absolute;
    z-index: 3;
    top: 8rpx;
    right: 0;
    bottom: 0;
    left: 0;
    height: 600rpx;
    padding-top: 132rpx;
}

.out-bg {
    width: 654rpx;
    height: 600rpx;
    margin-top: 8rpx;
}

.out .main-title-2 {
    margin: 16rpx auto;
    position: relative;
    display: inline-block;
    padding: 0 10px;
    margin-bottom: 86rpx;
}

.main-title-2::before,.main-title-2::after {
    content: '';
    position: absolute;
    top: 50%;
    height: 1px;
    width: 20px;
    background: #ffbbbb;
    transform: translate(0,-50%);
}

.main-title-2::before {
    left: -20px;
}

.main-title-2::after {
    right: -20px;
}

.out .btn {
    margin: 0 auto;
    width: 280rpx;
}

.out-bottom {
    position: absolute;
    bottom: 108rpx;
    left: 0;
    right: 0;
}

.main-footer {
    margin-top: -42rpx;
    position: relative;
    z-index: 2;
}

.seven-days {
    display: flex;
    padding: 0 20rpx;
    justify-content: space-between;
    align-items: center;
}

.day {
    position: relative;
    width: 84rpx;
    padding: 20px 0 0;
    text-align: center;
}

.day image {
    display: block;
    width: 84rpx;
    height: 112rpx;
    margin: 0 auto 10px;
}

.day-text {
    font-size: 10px;
    color: #666;
}

.day-text.old {
    color: #ccc;
}

.day-tips {
    position: absolute;
    z-index: 2;
    top: 0;
    left: 50%;
    height: 14px;
    line-height: 16px;
    padding: 0 6px;
    font-size: 10px;
    box-sizing: border-box;
    color: #fff;
    background: #FF464E;
    border-radius: 7px;
    white-space: nowrap;
    transform: translate(-50%,0);
}

.day-tips::after {
    content: '';
    position: absolute;
    bottom: -8px;
    left: 50%;
    border-width: 4px;
    border-style: solid dashed dashed;
    border-color: #FF464E transparent transparent;
    transform: translate(-50%,0);
}

.in {
    position: absolute;
    z-index: 3;
    top: 0;
    right: 0;
    bottom: 0;
    left: 0;
    padding-top: 60rpx;
}

.in.over {
    padding-top: 70rpx;
}

.in.fiveover {
    padding-top: 70rpx;
}

.in,.in-bg {
    height: 600rpx;
}

.in-bg {
    width: 658rpx;
}

.in .btn {
    width: 460rpx;
}

.in-bottom {
    position: absolute;
    right: 0;
    bottom: 98rpx;
    left: 0;
}

.bottom-tips {
    font-size: 0;
}

.in-bottom .bottom-tips {
    margin-bottom: 22rpx;
}

.bottom-tips-text {
    font-size: 28rpx;
    color: #fff;
}

.bottom-tips-text .em {
    color: #ffff66;
}

.angle {
    border-color: #ffff66;
    border-width: 4rpx 4rpx 0 0;
    transform: rotate(45deg) scale(.9);
}

.enters {
    display: flex;
    margin: 16rpx 86px 0;
    justify-content: center;
    align-items: center;
}

.enters navigator,.enters text {
    flex: 1;
    line-height: 40rpx;
    font-size: 24rpx;
    color: #ffd083;
}

.count {
    display: flex;
    justify-content: center;
    align-items: flex-end;
    color: #fff;
    line-height: 56rpx;
}

.five .count {
    margin-top: 15rpx;
}

.count-text {
    font-size: 84rpx;
    font-weight: bold;
}

.count-unit {
    margin-left: 8rpx;
    font-size: 36rpx;
    font-weight: 400;
}

.count-concat {
    margin: 0 8rpx;
    font-size: 72rpx;
    line-height: 1;
}

.five-tips {
    display: block;
    margin: 8rpx 0 10rpx;
    font-size: 24rpx;
    color: #fff;
}

.in.fiveover .five-tips {
    display: block;
    margin: 8rpx 0 30rpx;
    font-size: 24rpx;
    color: #fff;
}

.count+.five-tips {
    margin-top: 0;
}

.count+.btn {
    margin-top: 16rpx;
}

.alert-bg {
