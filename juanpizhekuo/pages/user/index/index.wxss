@import "../../../components/blockAds/index.wxss";

page,.content {
    border-top: none;
    width: 100%;
    height: 100%;
    position: absolute;
    left: 0;
    top: 0;
    right: 0;
    overflow-x: hidden;
    background-color: #f4f4f8;
    z-index: -99;
}

.header {
    color: #fff;
    text-align: center;
    font-size: 22px;
    height: 122px;
    width: 100%;
    overflow-x: hidden;
    position: relative;
}

.header .header_bc {
    overflow: hidden;
    height: 122px;
    width: 100%;
    position: absolute;
    top: 0;
    left: 0;
}

.header .header_bc .background_img {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    z-index: -1;
}

.header .headImg {
    float: left;
    width: 54px;
    height: 54px;
    border-radius: 50%;
    background-color: #eee;
    margin: 34px 0 0 30px;
    overflow: hidden;
}

.header .headImg .image_user {
    width: 100%;
    height: 100%;
}

.header .message {
    position: absolute;
    width: 100%;
    height: 122px;
    left: 85px;
    top: 0px;
    box-sizing: border-box;
    padding: 0 0 0 14px;
    overflow: hidden;
    float: left;
}

.login,.notLogin {
    width: 100%;
    height: 100%;
    display: none;
}

.login_show {
    display: block;
}

.notLogin_show {
    display: block;
}

.notLogin .goLogin {
    width: 60px;
    height: 24px;
    line-height: 24px;
    color: #fff;
    font-size: 14px;
    border: 1px solid #fff;
    border-radius: 2px;
    text-align: center;
    box-sizing: border-box;
}

.login {
    padding-top: 40px;
}

.notLogin {
    padding-top: 47px;
}

.Infor_name {
    display: flex;
    align-items: center;
    font-size: 16px;
    height: 22px;
    line-height: 22px;
    color: #FFFFFF;
    text-align: left;
}

.Info_address {
    font-size: 12px;
    color: #FFFFFF;
    height: 17px;
    line-height: 17px;
    margin-top: 8px;
    text-align: left;
    position: relative;
}

.change_address {
    width: 12px;
    height: 12px;
    position: absolute;
    left: 53px;
    top: 2px;
}

.change_address image {
    width: 100%;
    height: 100%;
}

.cell-row {
    border-width: 1px 0;
    line-height: 48px;
    background: #fff;
    padding: 0 15px;
    position: relative;
    color: #333;
}

.cell-row__footer {
    color: #888;
    position: absolute;
    right: 26px;
    top: 50%;
    transform: translateY(-50%);
}

.cell-row .angle {
    color: #999;
}

.cell-row .text {
    color: #999;
    font-size: 12px;
}

.cell-row::after {
    display: block;
    content: '';
    width: 92.6%;
    height: 1px;
    position: absolute;
    bottom: 0;
    left: 3.7%;
    background-color: #E5E5E5;
}

.border_none::after {
    display: none;
}

.custom_nav {
    width: 100%;
    overflow: hidden;
    height: 75px;
    box-sizing: border-box;
    background-color: #fff;
}

.custom_nav .button {
    width: 25%;
    height: 100%;
    float: left;
    text-align: center;
    box-sizing: border-box;
    position: relative;
}

.custom_nav .button .button_text {
    width: 100%;
    height: 17px;
    line-height: 17px;
    color: #666;
    font-size: 12px;
}

.custom_nav .button .button_imgge {
    width: 28px;
    height: 28px;
    margin: 12px 0 6px 0;
}

.sup {
    position: absolute;
    top: 4px;
    left: 59%;
    width: auto;
    min-width: 9px;
    padding: 0 3px;
    height: 15px;
    line-height: 16px;
    text-align: center;
    background-color: #ff464e;
    border-radius: 13px;
    color: #fff;
    font-size: 12px;
    display: none;
    box-sizing: content-box;
}

.sup_show {
    display: block;
}

.notice {
    background: rgba(255,153,0,0.1);
    display: flex;
    justify-content: center;
    align-items: center;
    padding: 0 15px;
    height: 32px;
}

.notice .laba {
    width: 24px;
    height: 24px;
}

.notice .txt {
    flex: 1;
    overflow: hidden;
    text-overflow: ellipsis;
    padding-left: 10px;
    font-size: 12px;
    color: #ff7700;
    white-space: nowrap;
    word-wrap: none;
    height: 32px;
    line-height: 32px;
}

.notice .angle {
    color: #ff7700;
}

.new_mark {
    position: absolute;
    display: block;
    width: 64rpx;
    height: 28rpx;
    left: 145rpx;
    top: 18rpx;
}

.vip {
    width: 242rpx;
    height: 170rpx;
    position: absolute;
    bottom: -15px;
    right: -10px;
    transform: rotate(10deg);
    z-index: 10;
}

.vip view,.vip image {
    width: 242rpx;
    height: 170rpx;
}

.new-tag {
    display: flex;
    align-items: center;
    justify-content: center;
    width: 32px;
    height: 14px;
    padding-top: 1px;
    border-radius: 10px;
    font-size: 10px;
    color: #fff;
    background-color: #ff464e;
}

.vip-tag {
    display: inline-block;
    line-height: 14px;
    padding-left: 8px;
    padding-right: 8px;
    font-size: 11px;
    color: #ff464e;
    background-color: #fff;
    border-radius: 100px;
    margin-left: 8px;
}

.money {
    color: #FF464E;
    font-size: 23rpx;
}

.texts-desc {
    display: block;
    font-size: 21rpx;
    text-align: left;
    color: #fff;
    margin-top: 10rpx;
}

.ad_content {
    margin-top: 25rpx;
    background-color: #fff;
    padding: 15px;
}