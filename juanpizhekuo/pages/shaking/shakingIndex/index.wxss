@import "../../../components/qipao/qipao.wxss";

@import "../../../components/goods/goods.wxss";

@import "../../../components/send-formid/index.wxss";

@import "../../../components/backtop/backtop.wxss";

@import "../../../components/countdown/countdown.wxss";

page {
    width: 100%;
    background-color: #FFC719;
    padding: 0;
    margin: 0;
    border: 0;
}

img {
    margin: 0;
    padding: 0;
}

::-webkit-scrollbar {
    background-color: transparent;
    width: 1px;
    height: 1px;
}

.goods-container {
    background-color: #f4f4f8;
}

.grouping-title {
    width: 100%;
    height: 88rpx;
    line-height: 88rpx;
    color: #fff;
    font-size: 32rpx;
    background-color: #FD4E40;
    text-align: center;
}

.goIndex {
    width: 192rpx;
    height: 56rpx;
    position: fixed;
    right: 0;
    top: 825rpx;
    z-index: 9999;
}

.goIndex .goIndexImg {
    width: 100%;
    height: 100%;
}

.qipao2 {
    background: rgba(255,255,255,0.6);
    font-size: 24rpx;
    color: #FD4E40;
    height: 40rpx;
    line-height: 40rpx;
    border: none;
    border-radius: 0 20rpx 20rpx 0;
}

.content_empty {
    width: 100%;
    height: 806rpx;
    position: relative;
}

.content_empty_bkImg {
    width: 100%;
    height: 100%;
    position: absolute;
    top: 0;
    left: 0;
    z-index: -1;
}

.personall_empty {
    width: 100%;
    text-align: center;
    height: 56rpx;
    line-height: 56rpx;
    position: absolute;
    top: 654rpx;
    left: 0;
    z-index: 5;
    font-size: 40rpx;
    color: #B73E30;
}

.payAttention {
    width: 100%;
    text-align: center;
    height: 36rpx;
    line-height: 36rpx;
    position: absolute;
    top: 738rpx;
    left: 0;
    z-index: 1;
}

.payAttention_tips {
    font-size: 24rpx;
    color: #FCEBC8;
    float: left;
    margin-left: 170rpx;
}

.payAttention_click {
    float: left;
    width: 132rpx;
    font-size: 20rpx;
    color: #FCEBC8;
    height: 36rpx;
    line-height: 36rpx;
    box-sizing: border-box;
    border-radius: 18rpx;
    margin-left: 16rpx;
}

.content_gameStart {
    width: 100%;
    height: auto;
    min-height: 900rpx;
    position: relative;
    padding-top: 48rpx;
}

.content_gameStart_bkImg {
    position: absolute;
    width: 100%;
    height: 874rpx;
    top: 0;
    left: 0;
    z-index: -1;
}

.circleIn {
    position: absolute;
    width: 444rpx;
    height: 448rpx;
    position: absolute;
    top: 272.8rpx;
    left: 50%;
    transform: translateX(-50%);
    z-index: 1;
}

.circleIn .circleInImg,.circleOut .circleOutImg,.shakeHands .shakeHandsImg,.longTips .longTipsImg {
    width: 100%;
    height: 100%;
}

.shakeHands {
    width: 304rpx;
    height: 396rpx;
    position: absolute;
    top: 290rpx;
    left: 50%;
    transform: translateX(-50%);
    z-index: 2;
    transform-origin: right bottom;
    -webkit-animation: hand_shake 500ms infinite linear;
}

@-webkit-keyframes hand_shake {
    from {
        transform: translateX(-50%) rotate(0deg);
    }

    50% {
        transform: translateX(-50%) rotate(15deg);
    }

    to {
        transform: translateX(-50%) rotate(0deg);
    }
}

@keyframes hand_shake {
    from {
        transform: translateX(-50%) rotate(0deg);
    }

    50% {
        transform: translateX(-50%) rotate(15deg);
    }

    to {
        transform: translateX(-50%) rotate(0deg);
    }
}

.circleOut {
    position: absolute;
    width: 496rpx;
    height: 498rpx;
    position: absolute;
    top: 228rpx;
    left: 50%;
    transform: translateX(-50%);
    z-index: 3;
}

.longTips {
    position: absolute;
    width: 598rpx;
    height: 106rpx;
    position: absolute;
    top: 638rpx;
    left: 50%;
    transform: translateX(-50%);
    z-index: 4;
}

.headTextContent {
    width: 100%;
    height: auto;
    text-align: center;
    margin-bottom: 24rpx;
}

.headTextContent .haedText1 {
    width: 414rpx;
    height: 58rpx;
}

.headTextContent .haedText2 {
    width: 532rpx;
    height: 84rpx;
}

.content_main {
    width: 560rpx;
    height: auto;
    margin: 0 auto;
    min-height: 600rpx;
    padding-top: 68rpx;
    position: relative;
}

.wxImgContent {
    position: absolute;
    top: 0;
    left: 50%;
    transform: translateX(-50%);
    width: 128rpx;
    height: 128rpx;
    border-radius: 50%;
    box-sizing: content-box;
    border: 8rpx solid #FD4E40;
    overflow: hidden;
    z-index: 1;
}

.wxImg {
    width: 100%;
    height: 100%;
}

.readEnvelop {
    width: 100%;
    height: auto;
    background: #FD4E40;
    border-radius: 20px;
    box-sizing: border-box;
    padding: 80rpx 0 48rpx 0;
}

.tellTips {
    width: 100%;
    height: 50rpx;
    line-height: 50rpx;
    font-size: 36rpx;
    color: #F3E3B8;
    font-weight: bold;
    text-align: center;
    margin: 0 0 21rpx 0;
}

.prizePicContent {
    width: 100%;
    height: auto;
    overflow: hidden;
    min-height: 257rpx;
    margin: 0 auto;
}

.prizePicContentImg {
    width: 100%;
    height: 100%;
    position: relative;
}

.status1 {
    width: 100%;
    height: 278rpx;
}

.status2 {
    width: 100%;
    height: 298rpx;
}

.status4,.status5 {
    width: 100%;
    height: 286rpx;
}

.status2_3Tips {
    position: absolute;
    top: 88rpx;
    left: 50%;
    transform: translateX(-50%);
    height: 156rpx;
    width: 306rpx;
    line-height: 156rpx;
    font-size: 48rpx;
    color: #F8E7B9;
    font-weight: bold;
    text-align: center;
}

.status3_Tips {
    font-size: 64rpx;
    color: #F8E7B9;
}

.hb_time {
    width: 100%;
    height: 40rpx;
    line-height: 40rpx;
    text-align: center;
    font-size: 32rpx;
    color: #F1E0AD;
    margin-top: 20rpx;
}

.time__hours,.time__minutes,.time__seconds {
    display: inline-block;
    height: 40rpx;
    line-height: 40rpx;
    font-size: 28rpx;
    color: #B42F22;
    background-color: #F1E0AD;
    border-radius: 4rpx;
    padding: 0 6rpx;
    position: relative;
}

.time__seconds {
    width: 68rpx;
    padding-right: 20rpx;
    box-sizing: border-box;
}

.time__seconds::after {
    content: "";
    position: absolute;
    right: 6rpx;
    top: 50%;
    transform: translateY(-50%);
    width: 16rpx;
    height: 36rpx;
    background-image: url(https://jp.juancdn.com/wxMapp/yaoyiyao/time.gif);
    background-repeat: no-repeat;
    background-size: 100% 100%;
}

.clickBtn {
    width: 360rpx;
    height: 88rpx;
    text-align: center;
    line-height: 88rpx;
    border-radius: 44rpx;
    margin: 34rpx auto 0;
    background: #F5D79B;
    font-size: 36rpx;
    color: #B42F22;
    font-weight: bold;
    animation: twinkling 1s infinite linear;
}

.tipsTexts {
    width: 100%;
    margin-top: 16rpx;
    text-align: center;
    font-size: 24rpx;
    color: #F1E0AD;
}

.cutMoneyTip .MoneyCut {
    font-weight: bold;
    color: #FFEC00;
}

.cutMoneyTipShared {
    width: 336rpx;
    font-size: 24rpx;
    color: #F1E0AD;
    margin: 40rpx auto 0;
}

.cutMoneyTipShared .MoneyCut {
    color: #FFEC00;
}

.cutMoneyTipShared .blueText {
    color: #0F53A1;
}

.activityRules {
    width: 100%;
    height: 34rpx;
    line-height: 34rpx;
    text-align: center;
    font-size: 24rpx;
    color: #5A0E06;
    margin-top: 20rpx;
}

.payAttention2_out {
    width: 100%;
    text-align: center;
    height: 60rpx;
    line-height: 60rpx;
    font-weight: bold;
    margin-top: 48rpx;
    background: #FF8076;
    padding-top: 12rpx;
}

.payAttention2 {
    width: 100%;
    height: 36rpx;
    line-height: 36rpx;
}

.payAttention_tips2 {
    font-size: 24rpx;
    color: #FCEBC8;
    float: left;
    margin-left: 163rpx;
}

.payAttention_click2 {
    float: left;
    width: 132rpx;
    font-size: 24rpx;
    color: #FCEBC8;
    height: 34rpx;
    line-height: 34rpx;
    border: 2rpx solid #FCEBC8;
    border-radius: 18rpx;
    margin-left: 24rpx;
}

.overTimeWindowContent,.jpCode_content,.rules_content {
    width: 100%;
    height: 100%;
    background-color: rgba(0,0,0,0.7);
    position: fixed;
    z-index: 9999999999999;
    top: 0;
    left: 0;
}

.overTimeWindow {
    width: 612rpx;
    min-height: 606rpx;
    height: auto;
    background: #fff;
    border-radius: 20rpx;
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%,-50%);
    box-sizing: border-box;
    padding: 48rpx 66rpx 60rpx;
    position: relative;
}

.overTimeTips {
    font-size: 32rpx;
    color: #000000;
    text-align: center;
    font-weight: bold;
}

.overTimePrize {
    width: 440rpx;
    height: 266rpx;
    margin: 32rpx auto;
}

.overTimePrize .overTimePrizeImg {
    width: 100%;
    height: 100%;
}

.go60devide {
    width: 436rpx;
    height: 90rpx;
    line-height: 90rpx;
    text-align: center;
    color: #fff;
    font-size: 36rpx;
    background: #FD4E40;
    border-radius: 45rpx;
    margin: 0 auto;
}

.windowClose {
    width: 60rpx;
    height: 114rpx;
    position: absolute;
    right: 19rpx;
    top: -113rpx;
    text-align: center;
}

.windowClose .closeImg {
    width: 100%;
    height: 100%;
}

.jpcode_block {
    background: #fff;
    border-radius: 8px;
    width: 612rpx;
    height: 606rpx;
    position: absolute;
    left: 50%;
    top: 50%;
    transform: translate(-50%,-50%);
    box-sizing: border-box;
    padding: 64rpx 100rpx 60rpx 100rpx;
}

.jpCode {
    width: 412rpx;
    height: 412rpx;
    margin: 0 auto;
    overflow: hidden;
}

.jpCode_image {
    width: 100%;
    height: 100%;
}

.jpcode_text {
    width: 100%;
    text-align: center;
    height: 50rpx;
    line-height: 50rpx;
    font-size: 36rpx;
    color: #FD4E40;
    margin-top: 20rpx;
}

.rule_box {
    color: #fff;
    position: absolute;
    width: 612rpx;
    height: 780rpx;
    background: #fff;
    box-shadow: 0 4rpx 32rpx 0 rgba(0,0,0,0.90);
    border-radius: 16rpx;
    top: 40%;
    left: 50%;
    transform: translate(-50%,-40%);
    box-sizing: border-box;
    padding: 48rpx 24rpx 60rpx 24rpx;
}

.view_head {
    width: 100%;
    height: 50rpx;
    line-height: 50rpx;
    font-size: 36rpx;
    margin-bottom: 24rpx;
    text-align: center;
    font-weight: bold;
    color: #333333;
}

.rules_all {
    width: 100%;
    list-style: none;
    height: 616rpx;
    font-size: 28rpx;
    color: #333333;
    line-height: 44rpx;
}

.rules_all view {
    display: block;
    width: 100%;
    font-size: 28rpx;
    line-height: 44rpx;
}

.rule_close,.coupon_close {
    width: 60rpx;
    height: 114rpx;
    position: absolute;
    right: 19rpx;
    top: -113rpx;
    text-align: center;
}

.rule_close image {
    width: 100%;
    height: 100%;
}

.back-top {
    position: fixed;
    bottom: 12px;
    right: 12px;
    z-index: 3;
    width: 46px;
    height: 50px;
    transition: all 0.5s ease;
}

.back-top image {
    width: 100%;
    height: 100%;
}

.backtop-true {
    opacity: 0;
    transform: translateY(100%);
}

.backtop-false {
    opacity: 1;
    transform: translateY(0);
}

@-webkit-keyframes twinkling {
    from {
        transform: scale3d(1,1,1);
    }

    50% {
        transform: scale3d(1.05,1.05,1.05);
    }

    to {
        transform: scale3d(1,1,1);
    }
}

@keyframes twinkling {
    from {
        transform: scale3d(1,1,1);
    }

    50% {
        transform: scale3d(1.05,1.05,1.05);
    }

    to {
        transform: scale3d(1,1,1);
    }
}