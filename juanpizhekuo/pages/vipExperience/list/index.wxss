@import "../../../components/loadmore1/loadmore.wxss";

@import "../../../components/send-formid/index.wxss";

page,.container {
    display: flex;
    width: 100%;
}

.topProgress {
    width: 100%;
    height: 56rpx;
}

.navStyle {
    font-size: 0px;
}

.topBannerBG {
    position: relative;
    width: 100%;
    height: 592rpx;
}

.topBannerBtn {
    position: absolute;
    height: 126rpx;
    left: 100rpx;
    right: 100rpx;
    bottom: 20rpx;
}

.out_container {
    display: flex;
    width: 100%;
    background-color: #f4f4f8;
}

.listContainer {
    font-size: 0;
    width: 100%;
    display: flex;
    justify-content: center;
    flex-wrap: wrap;
    background-color: #ff7057;
}

.item {
    width: 100%;
}

.itemContainer {
    display: flex;
    width: 100%;
    padding-left: 20rpx;
    padding-right: 20rpx;
    padding-top: 20rpx;
    flex-direction: row;
}

.wrapper {
    position: relative;
    height: 288rpx;
    display: flex;
    flex: 1;
    flex-direction: row;
    background-color: white;
    padding: 24rpx;
    border-radius: 4px;
}

.formStyle {
    position: absolute;
    width: 100%;
    height: 100%;
    left: 0;
    right: 0;
    top: 0;
    bottom: 0;
    background: transparent;
}

.formBtnStyle {
    width: 100%;
    height: 100%;
    background: transparent;
}

.formBtnStyle::after {
    border: 0px solid #CC3333;
}

.image {
    width: 240rpx;
    height: 240rpx;
    position: relative;
}

.cornerImage {
    position: absolute;
    width: 74rpx;
    height: 74rpx;
    left: 0;
    top: 0;
}

.bottom {
    display: flex;
    justify-content: flex-end;
    flex-direction: column;
    height: 100%;
    width: 100%;
}

.rightContainer {
    display: flex;
    width: 100%;
    height: 100%;
    margin-left: 20rpx;
    flex-direction: column;
}

.titleText {
    color: #333;
    font-size: 14px;
    overflow: hidden;
    white-space: normal;
    width: 100%;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 2;
}

.subTitle {
    color: #999;
    font-size: 12px;
    background-color: transparent;
    margin-top: 10rpx;
}

.price {
    font-size: 24px;
    color: #ff464e;
    font-weight: bold;
}

.bottomContainer {
    width: 100%;
    display: flex;
    justify-content: space-between;
    flex-direction: row;
    align-items: center;
}

.btn {
    display: flex;
    justify-content: center;
    align-items: center;
    height: 60rpx;
    background-color: #ff464e;
    border-radius: 100px;
    flex-direction: row;
    border-width: 0px;
    padding-left: 30rpx;
    padding-right: 30rpx;
}

.btnText {
    color: #fff;
    font-size: 14px;
}

.load-more {
    line-height: 1;
    padding: 32rpx 0;
    color: #ffe9e6;
    text-align: center;
    font-size: 14px;
    display: block;
    clear: both;
}

.arrow__view {
    transform: rotate(45deg);
    height: 8px;
    width: 8px;
    border-width: 1px 1px 0 0;
    border-style: solid;
    border-color: #fff;
}

.dialog_bg {
    position: fixed;
    left: 0;
    top: 0;
    width: 100%;
    height: 100%;
    background-color: rgba(0,0,0,0.45);
    z-index: 2;
    padding-left: 70rpx;
    padding-right: 70rpx;
}

.dialog_content {
    display: flex;
    flex-direction: column;
    width: 100%;
    align-items: flex-end;
    margin-top: 200rpx;
}

.dialog_content_wrapper {
    display: flex;
    flex-direction: column;
    position: relative;
    width: 100%;
    height: 430rpx;
    border-radius: 10rpx;
    background-color: white;
    align-items: center;
}

.closeBtn {
    width: 60rpx;
    height: 114rpx;
    margin-right: 20rpx;
}

.dialog_title_text {
    color: #ff464e;
    font-size: 18px;
    margin-top: 48rpx;
    font-weight: bold;
}

.dialog_desc_text {
    color: #333;
    font-size: 14px;
    margin-top: 20rpx;
    margin-left: 62rpx;
    margin-right: 62rpx;
}

.bottom_wrapper {
    display: flex;
    flex-direction: row;
    height: 128rpx;
    margin-top: 40rpx;
}

.bottom_btn_desc {
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
    width: 304rpx;
    height: 100%;
    border: 1px #ff464e solid;
    border-radius: 10rpx 0 0 10rpx;
    background-color: #fff9f9;
}

.bottom_btn {
    width: 128rpx;
    height: 100%;
    display: flex;
    justify-content: center;
    align-items: center;
    background-color: #ff464e;
    border-radius: 0 10rpx 10rpx 0;
    color: white;
}

.bottom_btn_desc_title {
    color: #333;
    font-size: 12px;
}

.bottom_btn_desc_subtitle {
    font-size: 16px;
    color: #ff464e;
    font-weight: bold;
}

.achieve_success_save_money {
    display: flex;
    flex-direction: row;
    color: #333;
    font-size: 24px;
    margin-top: 8rpx;
    align-items: center;
    justify-content: center;
    font-weight: bold;
}

.achieve_success_btn {
    display: flex;
    justify-content: center;
    align-items: center;
    width: 436rpx;
    height: 90rpx;
    border-radius: 100px;
    color: white;
    font-size: 18px;
    margin-top: 60rpx;
    background-color: #ff464e;
}

.start_activity_bg {
    position: relative;
    display: flex;
    flex-direction: column;
    align-items: center;
    width: 100%;
    border-radius: 10rpx;
    background-color: white;
    padding-bottom: 64rpx;
}

.start_activity_bg__title {
    color: #333;
    margin-top: 48rpx;
    font-weight: bold;
    font-size: 18px;
    font-weight: bold;
}

.start_activity_bg__subtitle {
    color: #333;
    font-size: 16px;
    margin-top: 20rpx;
    margin-left: 50rpx;
    margin-right: 50rpx;
}

.start_activity_bg__bottom {
    width: 100%;
    display: flex;
    flex-direction: row;
    position: absolute;
    bottom: 0;
    height: 86rpx;
    border-width: 1px 0 0 0;
    border-style: solid;
    border-color: #ebebeb;
    font-size: 16px;
}

.start_activity_bg__leftbtn {
    display: flex;
    justify-content: center;
    align-items: center;
    flex: 1;
    height: 100%;
    color: #333;
}

.start_activity_bg__rightbtn {
    display: flex;
    justify-content: center;
    align-items: center;
    flex: 1;
    height: 100%;
    color: #ff464e;
    border-width: 0 0 0 1px;
    border-style: solid;
    border-color: #ebebeb;
}

.operations {
    border-radius: 0;
    border: none;
    background: transparent;
}

.operations::after {
    display: none;
}