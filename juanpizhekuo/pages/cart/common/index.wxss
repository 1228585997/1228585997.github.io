@import "../../../components/priceList/index.wxss";

.cart__win .head__title {
    height: 96rpx;
    background: #fff;
    font-size: 18px;
    color: #333;
    text-align: center;
    line-height: 96rpx;
    border-bottom: 1px solid #ebebeb;
}

.cart__scroll {
    margin: 0px 0px 50px 0px;
    background: #efefef;
    max-height: 900rpx;
}

.cart__empty__wrapper {
    margin: 120rpx 0 40rpx;
    text-align: center;
}

.cart__empty {
    text-align: center;
    font-size: 16px;
    color: #333;
}

.empty__icon_image {
    width: 140px;
    height: 140px;
}

.cart__empty__wrapper .login__btn {
    padding: 6px 40px;
    border: 1px solid #ff464e;
    font-size: 12px;
    color: #ff464e;
    margin-top: 10px;
    text-align: center;
    display: inline-block;
    border-radius: 2px;
}

.item__cart {
    margin-bottom: 20rpx;
}

.item__head {
    display: flex;
    height: 88rpx;
    justify-content: center;
    align-items: center;
    background: #fff;
    border-bottom: 1px solid #ebebeb;
}

.head__radio {
    padding: 0 20rpx;
    height: 48rpx;
}

.head__radio__icon {
    width: 48rpx;
    height: 48rpx;
}

.head__name {
    flex: 1;
}

.head__name__box {
    display: flex;
    justify-content: flex-start;
    align-items: center;
    width: 200px;
}

.head__name_icon {
    width: 40rpx;
    height: 40rpx;
    vertical-align: middle;
    margin-right: 8rpx;
}

.head__name__txt {
    color: #666;
    font-size: 12px;
    vertical-align: middle;
    max-width: 160px;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
}

.cart__arrow {
    display: inline-block;
    transform: rotate(45deg);
    height: 8px;
    width: 8px;
    border-width: 2px 2px 0 0;
    border-style: solid;
    margin-left: .2em;
    vertical-align: middle;
    color: #666;
}

.item__act_action .cart__arrow {
    margin-top: -2px;
}

.head__action {
    width: 280rpx;
    color: #ff464e;
    font-size: 12px;
    margin-right: 20rpx;
    text-align: right;
}

.item__act {
    display: flex;
    padding: 20rpx 0;
    justify-content: center;
    align-items: center;
    background: #fff;
}

.item__act+.good__list .good__item:first-child {
    padding-top: 0;
}

.item__act__info {
    flex: 1;
    margin-left: 88rpx;
    white-space: nowrap;
    text-overflow: ellipsis;
}

.act__tag {
    font-size: 10px;
    padding: 0 2px;
    border: 1px solid #ff464e;
    color: #ff464e;
    display: inline-block;
    margin-right: 16rpx;
    vertical-align: middle;
}

.act__info {
    font-size: 12px;
    color: #333;
    vertical-align: middle;
    display: inline-block;
    width: 180px;
    overflow: hidden;
    text-overflow: ellipsis;
}

.item__act_action {
    color: #ff464e;
    width: 120rpx;
    text-align: right;
    font-size: 12px;
    margin-right: 20rpx;
}

.red {
    color: #ff464e;
}

.good__list {
    background: #fff;
    padding-bottom: 20rpx;
}

.good__item {
    display: flex;
    justify-content: center;
    align-items: center;
    background: #fff;
    padding-top: 20rpx;
}

.good__radio {
    padding: 0 20rpx;
}

.good__radio__icon {
    width: 48rpx;
    height: 48rpx;
}

.good__body {
    flex: 1;
    display: flex;
    justify-content: center;
    padding-bottom: 20rpx;
}

.item__group .good__list .good__item:last-child .good__body {
    padding-bottom: 0;
}

.item__group .good__list .good__item .good__body:last-child {
    border-bottom: 1px solid #ebebeb;
}

.item__group .good__list .good__item:last-child .good__body:last-child {
    border-bottom: none;
}

.good__body_l {
    width: 180rpx;
    height: 180rpx;
}

.good__img {
    width: 180rpx;
    height: 180rpx;
    position: relative;
}

.good__body_r {
    flex: 1;
    margin: 0 30rpx;
    position: relative;
}

.good__info1 {
    display: flex;
    justify-content: center;
}

.good__name {
    flex: 1;
    font-size: 13px;
    color: #333;
}

.good__price {
    width: 160rpx;
}

.cprice {
    font-size: 14px;
    color: #ff464e;
    text-align: left;
    display: inline-block;
}

.oprice {
    font-size: 10px;
    color: #bbb;
    text-align: right;
    margin-top: 8rpx;
    text-decoration: line-through;
    display: inline-block;
}

.good__info2 {
    font-size: 12px;
    color: #999;
}

.good__info3 {
    display: flex;
    justify-content: space-between;
    align-items: center;
    bottom: 0;
    width: 100%;
    position: absolute;
}

.good__count {
    position: absolute;
    right: 0;
    bottom: 0;
    flex: 1;
}

.good__count .inc {
    display: inline-block;
    background: #f7f7f7;
    width: 48rpx;
    height: 48rpx;
    line-height: 48rpx;
    text-align: center;
    font-size: 14px;
    color: #333;
    vertical-align: middle;
}

.good__count .num {
    display: inline-block;
    width: 58rpx;
    height: 48rpx;
    line-height: 48rpx;
    text-align: center;
    font-size: 14px;
    color: #333;
    vertical-align: middle;
}

.good__count .dec {
    display: inline-block;
    background: #f7f7f7;
    width: 48rpx;
    height: 48rpx;
    line-height: 48rpx;
    text-align: center;
    color: #333;
    vertical-align: middle;
}

.good__del {
    width: 48rpx;
}

.good__del__icon {
    width: 48rpx;
    height: 48rpx;
}

.good__count .dec.disabled,.good__count .inc.inc.disabled {
    color: #dbdbdb;
}

.cart__action {
    display: flex;
    height: 50px;
    justify-content: center;
    align-items: center;
    background: #fff;
    position: fixed;
    bottom: 0;
    width: 100%;
    z-index: 8;
}

.cart__radio {
    font-size: 16px;
    color: #333;
    padding: 0 20rpx;
}

.good__radio__icon {
    width: 48rpx;
    vertical-align: middle;
    text-align: center;
}

.cart__radio .good__radio__icon {
    margin-right: 20rpx;
}

.cart_info {
    flex: 1;
    padding-right: 20rpx;
}

.cart__count1 {
    font-size: 14px;
    color: #333;
    text-align: right;
}

.pay__amount {
    font-size: 14px;
    color: #ff464e;
    margin-left: 10rpx;
}

.cart__count2 {
    font-size: 10px;
    color: #ff464e;
    text-align: right;
}

.cart__btn {
    width: 240rpx;
    background: #ff464e;
    color: #fff;
    height: 50px;
    line-height: 50px;
    text-align: center;
}

.cart_limit_model {
    position: fixed;
    left: 0;
    top: 0;
    width: 100%;
    height: 100%;
    overflow: hidden;
    background-color: rgba(0,0,0,.45);
    z-index: 999;
}

.cart_limit_box {
    width: 590rpx;
    background-color: #fff;
    border-radius: 4px;
    overflow: hidden;
    position: absolute;
    left: 50%;
    top: 50%;
    transform: translate(-50%,-50%);
    color: #333333;
}

.cart_limit_box_title {
    font-size: 16px;
    width: 100%;
    height: 100rpx;
    border-bottom: 1px solid #ebebeb;
    box-sizing: border-box;
    line-height: 100rpx;
    text-align: center;
}

.cart_limit_box_content {
    width: 100%;
    max-height: 440rpx;
    overflow-y: scroll;
}

.cart_limit_box_btn {
    width: 100%;
    height: 100rpx;
    border-top: 1px solid #ebebeb;
    box-sizing: border-box;
    text-align: center;
    line-height: 100rpx;
}

.cart_limit_box_btn .cart_limit_box_btn_item {
    display: inline-block;
    width: 50%;
    height: 100%;
    font-size: 16px;
    border-right: 1px solid #ebebeb;
}

.cart_limit_box_btn .cart_limit_box_btn_right {
    color: #FF464E;
}

.cart_limit_box_btn .cart_limit_box_btn_noEntry {
    color: #DBDBDB;
}

.cart_limit_box_content_list {
    width: 100%;
    padding: 0 20rpx 20rpx;
}

.cart_limit_box_content_item {
    width: 100%;
    height: 140rpx;
    margin-top: 20rpx;
    overflow: hidden;
    position: relative;
}

.cart_limit_box_content_item .cart_limit_box_content_item_leftImg {
    height: 140rpx;
    width: 140rpx;
    float: left;
    margin-right: 20rpx;
}

.cart_limit_box_content_item .cart_limit_box_content_item_leftImg image {
    width: 100%;
    height: 100%;
    display: block;
}

.cart_limit_box_content_item .cart_limit_box_content_item_rightDes {
    float: left;
    width: 340rpx;
    height: 100%;
}

.cart_limit_box_content_item .cart_limit_box_content_item_rightDes_title {
    display: -webkit-inline-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 2;
    overflow: hidden;
    color: #333;
    word-wrap: break-word;
    word-break: break-all;
}

.cart_limit_box_content_item .cart_limit_box_content_item_rightDes_scale {
    font-size: 12px;
    color: #999999;
}

.cart_limit_box_content_item .cart_limit_box_content_item_rightDes_scale text {
    margin-right: 30rpx;
}

.cart_limit_box_content_item .cart_limit_box_content_item_num {
    font-size: 12px;
    color: #999999;
    position: absolute;
    right: 0;
    top: 50%;
    transform: translateY(-50%);
}

.cart_moq_info {
    text-align: right;
    color: #FF464E;
    font-size: 14px;
    margin-bottom: 4px;
}

.tip_container {
    position: fixed;
    bottom: 48px;
    z-index: 12;
    background-color: #464A5A;
    height: 40px;
    width: 100%;
    display: flex;
    align-items: center;
}

.tip_container .member_icon {
    margin-left: 14px;
    width: 32rpx;
}

.tip_container .member_txt {
    margin-left: 4px;
    color: #F7DFC4;
    font-size: 14px;
    text-align: left;
}

.tip_container .member_check {
    width: 16px;
    position: absolute;
    top: 12px;
    right: 16px;
}

.image_tip {
    width: 100%;
    height: 28rpx;
    opacity: 0.5;
    background-color: black;
    color: white;
    z-index: 11;
    position: absolute;
    bottom: 0;
    left: 0;
    font-size: 10px;
    line-height: 28rpx;
    text-align: center;
}

.leftMargin {
    margin-left: 3px;
    vertical-align: middle;
}