.failed-empty {
    background: #fff;
    position: fixed;
    display: flex;
    justify-content: center;
    align-items: center;
    width: 100%;
    height: 100%;
}

.status-img {
    text-align: center;
}

.failed-empty__image {
    display: inline-block;
    width: 280rpx;
    height: 280rpx;
}

.failed-text {
    margin: 18rpx 0 249rpx 0;
    text-align: center;
    font-size: 32rpx;
    color: #999999;
}